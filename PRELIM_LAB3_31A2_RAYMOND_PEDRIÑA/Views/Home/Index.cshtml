@model List<IDescribable>

<!DOCTYPE html>
<html>
<head>
    <style>
        body {
            background-color: lightblue;
            font-family: Arial, sans-serif;
        }
    </style>
</head>
<body>
    <h2>Customer List</h2>

    <ul> 
        @foreach (var customer in Model)
        {
            <li>@customer.Description()</li>
        }
    </ul>

    <hr />
</body>
</html>
